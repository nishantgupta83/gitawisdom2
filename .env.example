# GitaWisdom Environment Configuration
# Copy this file to .env and fill in your actual values
# NEVER commit .env to version control

# Supabase Configuration
SUPABASE_URL=your_supabase_project_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# App Environment (development, staging, production)
APP_ENV=development

# Feature Flags
ENABLE_ANALYTICS=false
ENABLE_LOGGING=true

# API Configuration
API_TIMEOUT_SECONDS=30

# Audio Configuration
AUDIO_BASE_URL=assets/audio/

# Build Instructions:
# Flutter build with environment variables:
# flutter build apk --dart-define=SUPABASE_URL=your_url --dart-define=SUPABASE_ANON_KEY=your_key
# flutter build ios --dart-define=SUPABASE_URL=your_url --dart-define=SUPABASE_ANON_KEY=your_key
